{
  "name": "vscode-line-filter",
  "displayName": "Line Filter",
  "description": "An extension for filtering the lines of a log file in an editor with configurable include/exclude patterns",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/roblourens/vscode-line-filter.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/vscode-wordcount/issues"
  },
  "contributes": {
    "commands": [
      {
        "command": "vscode-line-filter.filterActiveEditor",
        "title": "Filter Active Editor",
        "category": "Line Filter"
      },
      {
        "command": "vscode-line-filter.filterAndWatchActiveEditor",
        "title": "Filter and Watch Active Editor",
        "category": "Line Filter"
      },
      {
        "command": "vscode-line-filter.filterActiveEditorInPlace",
        "title": "Filter Active Editor In Place",
        "category": "Line Filter"
      }
    ],
    "configuration": {
      "properties": {
        "lineFilter.patterns": {
          "type": "object",
          "examples": [
            {
              "name": { "includes": ["includeThisSubstring"], "excludes": ["excludeThisOne"] }
            }
          ],
          "additionalProperties": {
            "type": "object",
            "properties": {
              "includes": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "excludes": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
